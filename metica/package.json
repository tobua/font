{
  "name": "metica",
  "description": "Rounded sans-serif font.",
  "version": "0.2.0",
  "repository": "github:tobua/font",
  "license": "MIT",
  "author": "Matthias Giger",
  "type": "module",
  "sideEffects": true,
  "main": "./metica.woff2",
  "types": "./types/metica.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./types/metica.d.ts",
        "default": "./metica.woff2"
      }
    },
    "./otf": {
      "default": {
        "types": "./types/metica-otf.d.ts",
        "default": "./metica.otf"
      }
    },
    "./ttf": {
      "default": {
        "types": "./types/metica-ttf.d.ts",
        "default": "./metica.ttf"
      }
    },
    "./woff": {
      "default": {
        "types": "./types/metica-woff.d.ts",
        "default": "./metica.woff"
      }
    },
    "./style": {
      "default": {
        "types": "./types/metica-style.d.ts",
        "default": "./metica.js"
      }
    }
  },
  "files": [
    "*",
    "types"
  ],
  "keywords": [
    "font",
    "metica",
    "web",
    "sans-serif",
    "rounded"
  ],
  "publishConfig": {
    "provenance": true
  }
}
