{
  "name": "tobua",
  "description": "Tobua brand specific custom font.",
  "version": "0.2.0",
  "repository": "github:tobua/font",
  "license": "MIT",
  "author": "Matthias Giger",
  "type": "module",
  "sideEffects": true,
  "main": "./tobua.woff2",
  "types": "./types/tobua.d.ts",
  "exports": {
    ".": {
      "default": {
        "types": "./types/tobua.d.ts",
        "default": "./tobua.woff2"
      }
    },
    "./otf": {
      "default": {
        "types": "./types/tobua-otf.d.ts",
        "default": "./tobua.otf"
      }
    },
    "./ttf": {
      "default": {
        "types": "./types/tobua-ttf.d.ts",
        "default": "./tobua.ttf"
      }
    },
    "./woff": {
      "default": {
        "types": "./types/tobua-woff.d.ts",
        "default": "./tobua.woff"
      }
    }
  },
  "files": [
    "*",
    "types"
  ],
  "keywords": [
    "font",
    "tobua",
    "web",
    "creative",
    "custom"
  ],
  "publishConfig": {
    "provenance": true
  }
}
